    "ansi-colors": "^4.1.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.2",
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.1",
    "mocha": "^6.1.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
